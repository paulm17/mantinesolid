    "postcss-preset-mantine": "1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "rollup": "^4.27.3",
    "rollup-plugin-banner2": "^1.3.0",
