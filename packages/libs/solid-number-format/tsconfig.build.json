{
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "esModuleInterop": true,
    "downlevelIteration": true,
    "noImplicitAny": true,
    "outDir": "./types",
    "declaration": true,
    "skipLibCheck": true,
    "emitDeclarationOnly": true
  },
  "include": ["src"]
}
